{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "fdvconverter",
  "version": "2.0.0",
  "identifier": "com.fdvconverter.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fdvconverter",
        "width": 800,
        "height": 900
        
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/Detectronic-DN/fdv_converter_t/releases/download/v2.0.0/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDM5NUJDMTkyQzcwOEQzRApSV1E5alhBc0dieVZBL3VaUmwvUHpGTGxHVGRLSDM5dkd5RW5xRjc0Wm41Mm1kVGdGNk1NNE5NUQo=",
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  }
}
